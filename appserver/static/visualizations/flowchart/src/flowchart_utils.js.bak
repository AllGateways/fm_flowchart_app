define([
    'jquery',
    'underscore',
    'jointjs',
    ], function(
        $,
        _,
        jointjs,
    ) {
        flowchart_diamond = jointjs.shapes.basic.Generic.extend({
        markup: '<g class="rotatable"><g class="scalable"><rect/></g><text/></g>',
        defaults: jointjs.util.deepSupplement({
            type: 'basic.Rect',
                attrs: {
                    'rect': { 
                        fill: 'lightgray', 
			stroke: 'black', 
			width: 0.5, 
			height: 0.5,
			transform: 'rotate(45)' },
             	     'text': { 
			'font-size': 8, 
			text: '', 
			'ref-x': .5, 
			'ref-y': .5, 
			ref: 'rect', 
			'y-alignment': 'middle', 
			'x-alignment': 'middle', 
			fill: 'lightgray', 
			'font-family': 'Arial, helvetica, sans-serif' }
        	}
    	    }, jointjs.shapes.basic.Generic.prototype.defaults)
	});
})
